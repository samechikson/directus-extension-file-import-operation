{
  "name": "directus-extension-file-import-operation",
  "description": "A Directus extension that adds a custom operation to import files from a remote URL.",
  "icon": "extension",
  "version": "1.1.1",
  "keywords": [
    "directus",
    "directus-extension",
    "directus-custom-operation"
  ],
  "type": "commonjs",
  "directus:extension": {
    "type": "operation",
    "path": {
      "app": "dist/app.cjs",
      "api": "dist/api.cjs"
    },
    "source": {
      "app": "src/app.ts",
      "api": "src/api.ts"
    },
    "host": "^9.0.0"
  },
  "scripts": {
    "build": "directus-extension build",
    "dev:docker": "docker compose up",
    "dev": "directus-extension build -w --no-minify",
    "link": "directus-extension link",
    "prepack": "npm run build",
    "release": "release-it"
  },
  "devDependencies": {
    "@directus/extensions-sdk": "^10.1.6",
    "@types/node": "^20.3.2",
    "release-it": "^16.1.2",
    "typescript": "^5.1.5",
    "vue": "^3.3.4"
  },
  "files": [
    "dist"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/samechikson/directus-extension-file-import-operation"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "release-it": {
    "github": {
      "release": true
    },
    "npm": {
      "publish": true
    }
  }
}
